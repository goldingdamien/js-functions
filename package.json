    "eslint": "^4.12.0",
    "eslint-html-reporter": "^0.5.2",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.0",
    "hamburgers": "^0.9.1",
    "istanbul": "^0.4.5",
    "jquery": "^3.2.1",
    "jsdom": "^11.1.0",
    "jsuri": "^1.3.1",
    "mocha": "^3.4.2",
